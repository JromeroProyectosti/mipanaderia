{{ form_start(form) }}
       	
<div class="card">
	<div class="card-body">

		<section>	
			
			<div class="row">
                <div class="col-sm-6 col-md-6">
                    <small class="text-muted">Nombre *</small>
                    {{form_row(form.nombre,{'label':false,'attr':{'class':'form-control'}})}}
                </div>
                <div class="col-sm-6 col-md-3">
                    <small class="text-muted">Rut </small>
                    {{form_row(form.rut,{'label':false,'attr':{'class':'form-control vRut'}})}}
                </div>
                <div class="col-sm-6 col-md-3">
                    <small class="text-muted">Contacto </small>
                    {{form_row(form.contacto,{'label':false,'attr':{'class':'form-control'}})}}
                </div>
                <div class="col-sm-6 col-md-3">
                    <small class="text-muted">Telefono </small>
                    {{form_row(form.telefono,{'label':false,'attr':{'class':'form-control vTelefono'}})}}
                </div>
                <div class="col-sm-6 col-md-3">
                    <small class="text-muted">Correo </small>
                    {{form_row(form.correo,{'label':false,'attr':{'class':'form-control vCorreo'}})}}
                </div>
                <div class="col-sm-6 col-md-3">
                    <small class="text-muted">Tipo Entidad </small>
                    {{form_row(form.tipoClienteProveedor,{'label':false,'attr':{'class':'form-control'}})}}
                </div>
            
            </div>
        </section>
    </div>
    <div class="card-footer" align="center">
        <button class="btn btn-primary">{{ button_label|default('Guardar') }}</button>
    </div>
</div>
{{ form_end(form) }}
